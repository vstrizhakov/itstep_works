<!DOCTYPE html>
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>Аромат | @ViewBag.Title</title>
	<link href="~/Content/bootstrap.min.css" type="text/css" rel="stylesheet" />
	@Scripts.Render("~/scripts/jquery-3.3.1.min.js")
	@Scripts.Render("~/scripts/jquery.unobtrusive-ajax.min.js")
</head>
<body>
	<nav class="navbar navbar-expand-lg navbar-dark bg-dark">
		<div class="container">
			<a class="navbar-brand" href="#">
				@Html.ActionLink("Аромат", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
			</a>
			<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
				<span class="navbar-toggler-icon"></span>
			</button>
			<div class="navbar-collapse collapse" id="navbarSupportedContent">
				<ul class="navbar-nav mr-auto">
					<li class="nav-item">
						@Html.ActionLink("Заказать кофе", "Index", "Home", new { area = "" }, new { @class = "nav-link" })
					</li>
					<li class="nav-item">
						@Html.ActionLink("Все заказы", "Orders", "Home", new { area = "" }, new { @class = "nav-link" })
					</li>
				</ul>
			</div>
		</div>
	</nav>
	<div class="container">
		<h2 class="my-4">@ViewBag.Title</h2>
		@RenderBody()
	</div>
</body>
</html>
